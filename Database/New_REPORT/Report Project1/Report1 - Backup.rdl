      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT        CLIENTS.first_name, CLIENTS.last_name, COURT_CASE.case_number, COURT_CASE.case_type, COURT_CASE.case_title, COURT_CASE.case_date, 
                         COURT_CASE.client_relationship, CASE_EMPLOYEE.HOURS_WORKED
FROM            CLIENTS INNER JOIN
                         COURT_CASE ON CLIENTS.client_ID = COURT_CASE.client_ID INNER JOIN
                         CASE_EMPLOYEE ON COURT_CASE.case_ID = CASE_EMPLOYEE.case_ID INNER JOIN
                         CASE_EXPENSE ON COURT_CASE.case_ID = CASE_EXPENSE.case_ID</CommandText>
